    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@types/node": "^20.17.7",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.13",
